#include <iostream>

using namespace std;

int main() {

   /* Type your code here. */
   string userInput; 
   int i;
 
   
   getline (cin, userInput);
   
   while (userInput != "Done" && userInput != "done" && userInput != "d") {
      for (i = userInput.size() - 1; i >= 0; i--) { //userInput.size() - 1 is required
         cout << userInput.at(i);
      }
      
      cout << endl;
      getline (cin, userInput);

      
      
   }
   
   return 0;
}
   
